  </servlet>
  <servlet-mapping>
  	<servlet-name>GetCanSignUpCompetitionServlet</servlet-name>
 	<url-pattern>/servlet/GetCanSignUpCompetitionServlet.do</url-pattern>
  </servlet-mapping>
</web-app>